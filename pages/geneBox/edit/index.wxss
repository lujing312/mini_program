page {
  background-color: #fff;
}
.container {
  background-color: #fff;
  padding: 40rpx;
  padding-bottom: 200rpx;
}
.title {
  font-size: 32rpx;
  line-height: 50rpx;
}
.num {
  margin-top: 10rpx;
  font-size: 32rpx;
  line-height: 50rpx;
}
.form-wrap {
  margin-top: 16rpx;
}
.form-item {
  font-size: 32rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #efeff5;
}
.form-item-flex-right {
  flex: 1;
  text-align: right;
}
.placeholder {
  color: #9b9b9b;
}
.icon-right-arrow {
  display: inline-block;
	font-size: 20rpx;
	color: #ccc;
}