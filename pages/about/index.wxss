page {
	background-color: #fff;
}
.container {
	background-color: #fff;
	padding-bottom: 200rpx;
}
.my-wrap {
	flex: 1;
	background-color: #fff;
	font-size: 28rpx;
	color: #484848;
}
.my-item {
	line-height: 72rpx;
	padding: 20rpx 40rpx;
	border-bottom: 1rpx solid #efeff5;
	display: flex;
	flex-direction: row;
	align-items: center;
  font-size: 32rpx;
}
.icon-right-arrow {
	font-size: 24rpx;
	color: #aaa;
}
.my-right-icon {
	flex: 1;
  text-align: right;
}
.my-item-right {
	flex: 1;
	display: flex;
  justify-content: flex-end;
  align-items: center;
}